{
  "env": {
    "browser": true,
    "es2021": true,
    "react-native/react-native": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "react-native"],
  "rules": {
    "semi": ["error", "never"],
    "quotes": ["error", "single"],
    "react/prop-types": ["off"]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
